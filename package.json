{
  "name": "fabula-ultima",
  "version": "0.0.0",
  "description": "Unofficial Roll20 Character Sheet for Fabula Ultima TTRPG",
  "scripts": {
    "start": "run-p compile:* postcss",
    "compile:sass": "npx sass ./src/scss/fabula-ultima.scss ./fabula-ultima.css --watch --no-source-map --no-charset",
    "compile:pug": "npx pug3 ./src/pug/fabula-ultima.pug --out ./ --watch --pretty",
    "postcss": "npx postcss fabula-ultima.css --output fabula-ultima.css --use autoprefixer --watch --verbose --no-map"
  },
  "keywords": [
    "roll20"
  ],
  "license": "MIT",
  "devDependencies": {
    "@anduh/pug-cli": "^1.0.0-alpha8",
    "@prettier/plugin-pug": "^2.3.0",
    "autoprefixer": "^10.4.7",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.14",
    "postcss-cli": "^9.1.0",
    "prettier": "^2.8.0",
    "pug": "^3.0.2",
    "pug-cli": "^1.0.0-alpha6",
    "sass": "^1.56.1"
  }
}
